{
  "name": "revys/revy-admin",,
  "description": "Admin Panel for Laravel Framework",
  "keywords": [
    "framework",
    "laravel",
    "revy",
    "revy-admin",
    "admin",
    "admin panel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Topalo",
      "email": "inwebcomp@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laravelcollective/html": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Revys\\RevyAdmin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Revys\\RevyAdmin\\Tests\\": "tests/",
      "Revys\\RevyAdmin\\Database\\Seeds\\": "src/database/seeds/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Revys\\RevyAdmin\\App\\Providers\\RevyAdminServiceProvider"
      ]
    }
  }
}
